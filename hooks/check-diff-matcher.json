{
    "problemMatcher": [
        {
            "owner": "check-diff",
            "pattern": [
                {
                    "regexp": "^\\*{3}\\s./(.+):(\\d+):\\s(.+):\\s(.+)$",
                    "file": 1,
                    "line": 2,
                    "message": 3,
                    "code": 4
                }
            ]
        }
    ]
}
